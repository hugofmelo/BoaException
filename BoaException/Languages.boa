#
# Lista os projetos Java
#
p: Project = input;

projectsNumber: output sum of int;
projects: output collection[string] of string;


visit(p, visitor {
    # look at Java-only projects  
	before n: Project ->
	{
	    ifall (i: int; !match(`^java$`, lowercase(n.programming_languages[i])))
	        stop;
		
		foreach (i: int; n.programming_languages[i])
			projects[p.name] << n.programming_languages[i];
			
		projectsNumber << 1;
	}
});